  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "mongoose": "^5.3.8"
  },
  "devDependencies": {
    "nodemon": "^1.18.5"
  }
